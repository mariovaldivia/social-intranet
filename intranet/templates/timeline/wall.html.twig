{% extends 'base.html.twig' %}

{% block title %}{{ user.profile ? user.profile : 'User is not registered'}}{% endblock %}

{% block body %}
    {% if user.profile %}

        {% if user.profile.imageFile %}
              <img src="{{ vich_uploader_asset(user.profile , 'imageFile')| imagine_filter('user') }}" 
					title="{{ user.profile }}" 
					/>
        {% endif %}
        
        {{ user.profile }}
    {% endif %}

    
{% endblock %}